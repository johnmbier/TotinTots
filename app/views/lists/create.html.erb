<h1>Tell us about your trip</h1>



<div class="question_form">
  <%= form_for(@list) do |f| %>
  <fieldset>
    <legend> How long is your trip?</legend>
    <div class="row">
      <div class="col-md-4">
       <select multiple class="form-control">
        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
       </select>
       <div class="actions">
        <%= f.submit %>
       </div>
     </div>
    </div>
    </fieldset>
  <% end %>
</div>
  